SQL: UPDATE `cl_content` SET `title`='Apple/苹果 iPhone 6s HK 海淘手机 玫瑰金 16GB',`keyword`='world！',`description`='',`content`='&lt;p&gt;uhjtrdshgfythgWERGAEHGSDBZCBDTYHAEHRTSHA&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;iframe class=&quot;ueditor_baidumap&quot; src=&quot;http://localhost/myblog/Public/UEditor/dialogs/map/show.html#center=113.93556,22.578345&amp;zoom=16&amp;width=530&amp;height=340&amp;markers=113.934949,22.577778&amp;markerStyles=l,A&quot; frameborder=&quot;0&quot; width=&quot;534&quot; height=&quot;344&quot; align=&quot;&quot;&gt;&lt;/iframe&gt;&lt;/p&gt;&lt;pre class=&quot;brush:php;toolbar:false&quot;&gt;/**
&amp;nbsp;*&amp;nbsp;判断是不是中文
&amp;nbsp;*&amp;nbsp;@author&amp;nbsp;刘紫华&amp;nbsp;2015-03-10
&amp;nbsp;*&amp;nbsp;@param&amp;nbsp;$str
&amp;nbsp;*/
function&amp;nbsp;checkChinese($str){
&amp;nbsp;&amp;nbsp;&amp;nbsp;if&amp;nbsp;(preg_match(&amp;quot;/^[\\x{4e00}-\\x{9fa5}]+$/u&amp;quot;,$str))&amp;nbsp;{
